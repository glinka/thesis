	number = {3},
	journaltitle = {{SIAM} Journal on Applied Dynamical Systems},
	shortjournal = {{SIAM} J. Appl. Dyn. Syst.},
	author = {Marschler, C. and Sieber, J. and Berkemer, R. and Kawamoto, A. and Starke, J.},
	urldate = {2017-08-10},
