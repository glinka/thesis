	file = {Full Text PDF:/home/holiday/Zotero/storage/TK2USP3I/Marschler et al. - 2014 - Implicit Methods for Equation-Free Analysis Conve.pdf:application/pdf;Snapshot:/home/holiday/Zotero/storage/A47HXACN/130913961.html:text/html}
}

@article{gross_epidemic_2006,
	title = {Epidemic Dynamics on an Adaptive Network},
