	keywords = {Bifurcation analysis, Complex systems, Individual-based epidemic models, Multiscale computations, Networks, Pair-wise correlations},
	file = {ScienceDirect Snapshot:/home/holiday/Zotero/storage/P86HKT4C/S009630031100748X.html:text/html;ScienceDirect Snapshot:/home/holiday/Zotero/storage/9CMD6FUF/S009630031100748X.html:text/html}
