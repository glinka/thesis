	month = feb,
	year = {2011},
	note = {arXiv: 1102.4639},
	keywords = {Computer Science - Social and Information Networks, Physics - Data Analysis, Statistics and Probability, Physics - Physics and Society},
	file = {arXiv\:1102.4639 PDF:/home/alexander/.zotero/zotero/sd5x9ptu.default/zotero/storage/W5AAI2AE/Ghosh et al. - 2011 - Non-Conservative Diffusion and its Application to .pdf:application/pdf;arXiv.org Snapshot:/home/alexander/.zotero/zotero/sd5x9ptu.default/zotero/storage/WJ6ABEUH/1102.html:text/html}
}

