	pages = {180--184},
	file = {Full Text PDF:/home/alexander/.zotero/zotero/sd5x9ptu.default/zotero/storage/XZSV3RBU/Eubank et al. - 2004 - Modelling disease outbreaks in realistic urban soc.pdf:application/pdf;Snapshot:/home/alexander/.zotero/zotero/sd5x9ptu.default/zotero/storage/G8ACTWFD/nature02541.html:text/html}
}

@article{ferguson_strategies_2005,
