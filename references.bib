	keywords = {Biological system modeling, biological systems, biology, Birds, commercial application, local agent actions, local agent-agent interactions, Lyapunov approach, Lyapunov methods, Microorganisms, military application, mobile robots, multi-agent systems, multiagent systems, multi-robot systems, networked cooperative uninhabited autonomous vehicles, noisy environment, Remotely operated vehicles, Robot kinematics, robots, Stability, stability analysis, stable social foraging swarms, swarm cohesiveness, Uncertainty, Working environment noise},
