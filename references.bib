	file = {Full Text PDF:/home/alexander/.zotero/zotero/sd5x9ptu.default/zotero/storage/XR9HAP9E/Ferguson et al. - 2005 - Strategies for containing an emerging influenza pa.pdf:application/pdf;Snapshot:/home/alexander/.zotero/zotero/sd5x9ptu.default/zotero/storage/F3SAPRM6/nature04017.html:text/html}
}

@article{fowlkes_spectral_2004,
